<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <properties resource="mybatis.properties"></properties>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="callSettersOnNulls" value="true"/>
        <setting name="lazyLoadingEnabled" value="false"/>
        <setting name="aggressiveLazyLoading" value="true"/>
        <setting name="multipleResultSetsEnabled" value="true"/>
        <setting name="useColumnLabel" value="true"/>
        <setting name="useGeneratedKeys" value="false"/>
        <setting name="autoMappingBehavior" value="PARTIAL"/>
        <setting name="defaultExecutorType" value="REUSE" />
        <setting name="defaultStatementTimeout" value="25000" />
        <setting name="jdbcTypeForNull" value="NULL"/>
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>

    <typeAliases>
        <typeAlias type="java.util.HashMap" alias="hashMap"/>
        <typeAlias type="java.lang.String" alias="String"/>
        <typeAlias type="com.architecture.admin.models.dto.member.MemberDto" alias="MemberDto"/>
        <typeAlias type="com.architecture.admin.models.dto.notification.MemberNotificationDto" alias="MemberNotificationDto"/>
        <typeAlias type="com.architecture.admin.models.dto.coin.CoinDto" alias="CoinDto"/>
        <typeAlias type="com.architecture.admin.models.dto.content.ContentDto" alias="ContentDto"/>
        <typeAlias type="com.architecture.admin.models.dto.episode.EpisodeDto" alias="EpisodeDto"/>
        <typeAlias type="com.architecture.admin.models.dto.rank.RankDto" alias="RankDto"/>
        <typeAlias type="com.architecture.admin.models.dto.statistics.CpStatDto" alias="CpStatDto"/>
        <typeAlias type="com.architecture.admin.models.dto.member.MemberGradeDto" alias="MemberGradeDto"/>
    </typeAliases>

    <mappers>
    </mappers>

</configuration>